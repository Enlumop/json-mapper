{
    "name": "enlumop/json-parser",
    "description": "Easy to use json parser",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Enlumop\\JsonParser\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Enlumop\\JsonParser\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "EnterV",
            "email": "kontakt@enterv.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "symfony/dotenv": "^6.3",
        "symfony/console": "^6.3",
        "phpunit/phpunit": "^10.3",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.34"
    },
    "scripts": {
        "analyse": "@phpstan",
        "phpstan": "vendor/bin/phpstan analyse tests src --level 9",
        "cs-fix": "vendor/bin/php-cs-fixer fix src",
        "test": "vendor/bin/phpunit tests",
        "checks": [
            "@test",
            "@phpstan"
        ]
    }
}